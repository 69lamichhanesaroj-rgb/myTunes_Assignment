<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="221.0" prefWidth="444.0"
            xmlns="http://javafx.com/javafx/25"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="easv.college.mytunesassignment.gui.NewPlaylistController">

    <children>
        <TextField fx:id="namePlaylist" layoutX="115.0" layoutY="71.0"
                   prefHeight="25.0" prefWidth="241.0"
                   promptText="Enter the name of the playlist" />

        <Label layoutX="60.0" layoutY="72.0" prefHeight="17.0"
               prefWidth="52.0" text="Name:">
            <font>
                <Font name="Lucida Bright" size="16.0" />
            </font>
        </Label>

        <Button layoutX="240.0" layoutY="157.0" mnemonicParsing="false"
                onAction="#btnCancelAction" prefHeight="25.0" prefWidth="72.0"
                text="Cancel" />

        <Button layoutX="327.0" layoutY="157.0" mnemonicParsing="false"
                onAction="#btnSaveAction" prefHeight="25.0" prefWidth="78.0"
                text="Save" />
    </children>

</AnchorPane>
